<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diajarin.id">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/diajarin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/diajarin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PaymentsActivity"></activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".DetailDiajarinMenjadi" />
        <activity android:name=".DetailDiajarinKerja" />
        <activity android:name=".VideoPlayerActivity" />
        <activity android:name=".DiajarinTesWebView" />
        <activity android:name=".DiajarinTesMulaiTes" />
        <activity android:name=".DiajarinTesTips" />
        <activity android:name=".DiajarinTesTanya" />
        <activity android:name=".DetailDiajarinTesActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true"/>

    </application>

</manifest>